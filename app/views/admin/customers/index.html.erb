<h3>会員一覧</h3>
<table>
<thaed>
  <tr>
    <th>会員ID</th>
    <th>氏名</th>
    <th>メールアドレス</th>

  </tr>
</thaed>
<tbody>

  <% @customer.each do |customer| %>
    <tr>
    <td><%= customer.id%></td>
    <td><%= link_to customer.first_name + customer.last_name, admin_customer_path(customer) %></td>
    <td><%= customer.email %></td>

    </tr>
  <% end %>
  </tbody>
  </table>